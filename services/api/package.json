{
  "name": "@auratechit/api",
  "version": "1.0.0",
  "description": "Main API service for AuraTechIT SaaS Platform",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
