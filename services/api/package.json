    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.9.7",
    "stripe": "^14.9.0",
    "redis": "^4.6.10",
    "bull": "^4.12.2",
    "prisma": "^5.6.0",
    "@prisma/client": "^5.6.0",
    "zod": "^3.22.4",
